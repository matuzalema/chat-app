{
  "name": "c",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "npm-run-all test:*",
    "test:html": "globstar nu-html-checker *.html",
    "init-project": "npm install && npm-run-all init:*",
    "init:dirs": "mkdirp sass css vendor images js",
    "init:files": "touch README.md index.html sass/style.scss js/script.js",
    "init:gitignore": "curl https://raw.githubusercontent.com/github/gitignore/master/Node.gitignore -o .gitignore",
    "build:sass": "node-sass --output-style extended -o css sass",
    "build:autoprefixer": "autoprefixer-cli css/style.css",
    "build-dev:sass": "node-sass --output-style expanded --source-map true -o css sass",
    "watch": "npm run build && npm-run-all -p watch:*",
    "watch:sassprefixer": "onchange sass -- npm-run-all build:sass build:autoprefixer",
    "watch:browser-sync": "browser-sync start --server --files \"css/*.css\" \"*.html\"",
    "build": "webpack --env production",
    "start": "webpack-dev-server --hot --env development"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer-cli": "^1.0.0",
    "babel-loader": "^8.0.5",
    "browser-sync": "^2.26.3",
    "globstar": "^1.0.0",
    "node-sass": "^4.10.0",
    "nu-html-checker": "^0.1.0",
    "onchange": "^5.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "html-webpack-plugin": "^3.2.0",
    "mkdirp": "^0.5.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.3",
    "optimize-js-plugin": "0.0.4",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "pug": "^2.0.3",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-hot-loader": "^4.8.4",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "description": ""
}
